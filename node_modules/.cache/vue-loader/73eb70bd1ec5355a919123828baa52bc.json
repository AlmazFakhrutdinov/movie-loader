nation />\n      </div>\n    </div>\n\n    <div class=\"movie-container\">\n      <div\n        class=\"movie box is-paddingless is-radiusless\"\n        v-for=\"(movie, index) in this.$store.state.movieByCategories\"\n        :key=\"index\"\n      >\n        <div class=\"is-flex\">\n          <div class=\"movie__img\">\n            <img :src=\"movie.poster_path\" alt />\n          </div>\n          <div class=\"movie__content is-flex\">\n            <div>\n              <div class>\n                <h6 class=\"title is-6\">{{ movie.original_title }}</h6>\n              </div>\n\n              <div>{{ getMonth(movie.release_date) }}</div>\n            </div>\n            <div class=\"movie-description\">\n              <p>{{ movie.overview.slice(0,213) }}</p>\n            </div>\n            <hr />\n            <div>\n              <p @click=\"movieInfo(movie.id)\" class=\"info\">More info</p>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <b-loading :is-full-page=\"true\" :active.sync=\"this.$store.state.loading\" :can-cancel=\"false\"></b-loading>\n  </div>\n</template>\n\n<script>\nimport Pagination from \"./pagination\";\n\nexport default {\n  components: { Pagination },\n  data() {\n    return {\n      checkboxGroup: [\"nowPlaying\", \"popular\", \"topRated\", \"upcoming\"],\n      movies: null,\n      pageNumber: 1\n    };\n  },\n  mounted() {\n    this.$store.dispatch(\"GET_MOVIE\", [this.pageNumber, this.checkboxGroup]);\n  },\n  methods: {\n    movieInfo(id) {\n      const route = this.$router.resolve({ path: `movieInfo/${id}` });\n      window.open(route.href, \"_blank\");\n    },\n    getMonth(item) {\n      let date = new Date(item);\n      let month = date.toLocaleString(\"default\", {\n        month: \"long\",\n        day: \"numeric\",\n        year: \"numeric\"\n      });\n      return month;\n    }\n  },\n  watch: {\n    checkboxGroup() {\n      this.$store.commit(\"GET_MOVIE\", [this.pageNumber, this.checkboxGroup]);\n    }\n  }\n};\n</script>\n\n<style scoped>\n.movie-container {\n  display: flex;\n  flex-direction: row;\n  flex-wrap: wrap;\n}\n.movie {\n  margin: 5px 13px 25px 13px;\n  width: 48%;\n}\n.movie__img {\n  width: 33%;\n  height: 320px;\n}\n.movie__img img {\n  width: 100%;\n  width: 100%;\n}\n.movie__content {\n  flex-direction: column;\n  width: 67%;\n  padding: 10px;\n}\n.movie-description {\n  font-size: 14px;\n  margin-top: 20px;\n}\nhr {\n  margin-top: auto;\n}\n.info {\n  margin-bottom: 10px;\n  font-size: 14px;\n}\n.movie-container:last-child {\n  height: 320px;\n}\n.nav {\n  margin: 20px 0px;\n}\n</style>"]}]}